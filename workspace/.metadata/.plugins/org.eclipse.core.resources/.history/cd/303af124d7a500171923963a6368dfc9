<%
	String num1 = request.getParameter("num1");
	String num2 = request.getParameter("num2");
	
	int op1 = Integer.parseInt(num1);
	int op2 = Integer.parseInt(num2);

	int add = op1 + op2 ;
	int diff = op1 + op2 ;
		
%>

<%= op1 %> + <%= op2 %> = <%= add %> <br>
<%= op1 %> - <%= op2 %> = <%= diff %> <br>



